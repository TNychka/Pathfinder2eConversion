{"name":"Darkleaf Breastplate","type":"armor","img":"systems/pf2e/icons/equipment/armor/leather-armor.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.4tIVTg9wj56RrveA"}},"_id":"rs3pgqiXaNmOQpPk","system":{"description":{"value":"<div class=\"flex-even column\" data-markdown=\"1\">\n<div class=\"content\" data-markdown=\"1\">\n<p class=\"hang\">Similar to leafweave armour, darkleaf armour is made of carefully cured and beautifully carved pieces of wood, supplemented by treated <span style=\"font-family:var(--font-primary);font-size:var(--font-size-14)\">leaves. This special process enables the many primal practitioners of the world a sturdy, yet entirely natural armour.</span></p>\n</div>\n</div>"},"source":{"value":"Pathfinder's Guide to Eberron"},"traits":{"value":["flexible","primal"],"rarity":"common","custom":"","otherTags":[]},"rules":[],"slug":null,"schema":{"version":0.812,"lastMigration":{"datetime":"2023-01-05T15:12:11.050-08:00","version":{"schema":0.743,"foundry":"10.291","system":"4.6.3"}}},"quantity":1,"baseItem":"leather-armor","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"1"},"equippedBulk":{"value":"1"},"price":{"value":{"gp":6}},"equipped":{"carryType":"worn","invested":null,"handsHeld":0,"inSlot":false},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Leather Armor","img":"systems/pf2e/icons/unidentified_item_icons/armor.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"wornarmor"},"level":{"value":0},"armor":{"value":2},"category":"light","group":null,"strength":{"value":12},"dex":{"value":3},"check":{"value":-1},"speed":{"value":0},"potencyRune":{"value":0},"resiliencyRune":{"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"va