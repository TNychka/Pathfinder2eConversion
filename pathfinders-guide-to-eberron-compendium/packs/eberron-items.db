{"name":"Boomerang, Talenta","type":"weapon","img":"systems/pf2e/icons/equipment/weapons/shuriken.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.7tKkkF8eZ4iCLJtp"}},"_id":"xZe26NMdZPwneyDf","system":{"description":{"value":"<p>The halflings of the Talenta Plains use traditional boomerangs - simple curved, polished sticks designed to return to the thrower.</p>"},"source":{"value":"Pathfinder's Guide to Eberron"},"traits":{"value":["agile","finesse","halfling","nonlethal","hb_returning","thrown-20"],"rarity":"uncommon","custom":"","otherTags":[]},"rules":[],"slug":null,"schema":{"version":0.812,"lastMigration":{"datetime":"2023-01-05T15:12:11.086-08:00","version":{"schema":0.743,"foundry":"10.291","system":"4.6.3"}}},"quantity":1,"baseItem":"hb_boomerang-talenta","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"1"},"equippedBulk":{"value":""},"price":{"value":{"gp":2}},"equipped":{"carryType":"worn","invested":null,"handsHeld":0,"-=inSlot":null},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Shortsword","img":"systems/pf2e/icons/unidentified_item_icons/weapon.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":0},"category":"martial","group":"hb_boomerang","bonus":{"value":0},"damage":{"value":"","dice":1,"die":"d4","damageType":"bludgeoning"},"bonusDamage":{"value":0},"splashDamage":{"value":0},"range":null,"reload":{"value":"-"},"MAP":{"value":""},"potencyRune":{"value":0},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":null},"propertyRune2":{"value":null},"propertyRune3":{"value":null},"propertyRune4":{"value":null},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":